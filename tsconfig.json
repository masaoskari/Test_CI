{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "jsx": "react",
    "module": "ES2022",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "./dist/",
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
    "strict": true,
    "target": "ES5",
    "sourceMap": true
  }
}
